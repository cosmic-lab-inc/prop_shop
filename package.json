{
  "name": "root",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*"
    ]
  },
  "scripts": {
    "clean": "yes | pnpm run clean && lerna clean -y && rm -rf .nx && rm -rf node_modules",
    "lint": "prettier --write \"./**/*.{ts,tsx}\" --loglevel silent",
    "prettify:fix": "pnpm run prettify:fix",
    "unlink:app-to-sdk": "cd packages/sdk && pnpm unlink && cd ../app && pnpm unlink @cosmic-lab/prop-shop-sdk && cd ../..",
    "link:app-to-sdk": "cd packages/sdk && pnpm link && cd ../app && pnpm link @cosmic-lab/prop-shop-sdk && cd ../..",
    "unlink:phoenix-vaults": "cd packages/sdk && pnpm unlink @cosmic-lab/phoenix-vaults-sdk && cd ../..",
    "link:phoenix-vaults": "cd packages/sdk && pnpm link @cosmic-lab/phoenix-vaults-sdk && cd ../..",
    "unlink:root-phoenix-vaults": "pnpm unlink @cosmic-lab/phoenix-vaults-sdk",
    "link:root-phoenix-vaults": "pnpm link @cosmic-lab/phoenix-vaults-sdk",
    "link:phoenix-vaults-program": "cp ../phoenix-vaults/target/idl/phoenix_vaults.json deps/phoenix_vaults.json && cp ../phoenix-vaults/target/deploy/phoenix_vaults.so deps/phoenix_vaults.so",
    "unlink:all": "pnpm unlink:app-to-sdk && pnpm unlink:root-phoenix-vaults && pnpm unlink:phoenix-vaults && pnpm unlink:phoenix-vaults-program",
    "link:all": "pnpm link:app-to-sdk && pnpm link:root-phoenix-vaults && pnpm link:phoenix-vaults && pnpm link:phoenix-vaults-program",
    "build:app": "pnpm --filter prop-shop-app build",
    "build:sdk": "pnpm --filter @cosmic-lab/prop-shop-sdk build",
    "build": "pnpm build:sdk && pnpm build:app",
    "rebuild": "pnpm clean && pnpm && pnpm build",
    "rebuild:dev": "pnpm clean && pnpm && pnpm link:all && pnpm dep:all && pnpm && pnpm build",
    "start": "pnpm --filter prop-shop-app start",
    "setup-target": "rm -rf target && mkdir target && mkdir target/deploy && mkdir target/idl",
    "dep:competitions": "cp deps/drift_competitions.so target/deploy/drift_competitions.so",
    "dep:drift-vaults": "cp deps/drift_vaults.json target/idl/drift_vaults.json && cp deps/drift_vaults.so target/deploy/drift_vaults.so",
    "dep:drift": "cp deps/drift.json target/idl/drift.json && cp deps/drift.so target/deploy/drift.so",
    "dep:phoenix": "cp deps/phoenix.so target/deploy/phoenix.so",
    "dep:phoenix-seat-manager": "cp deps/phoenix_seat_manager.so target/deploy/phoenix_seat_manager.so",
    "dep:phoenix-vaults": "cp deps/phoenix_vaults.json target/idl/phoenix_vaults.json && cp deps/phoenix_vaults.so target/deploy/phoenix_vaults.so",
    "dep:pyth": "cp deps/pyth.json target/idl/pyth.json && cp deps/pyth.so target/deploy/pyth.so",
    "dep:all": "pnpm dep:competitions && pnpm dep:drift-vaults && pnpm dep:drift && pnpm dep:pyth && pnpm dep:phoenix && pnpm dep:phoenix-seat-manager && pnpm dep:phoenix-vaults",
    "anchor-tests": "pnpm run ts-mocha -p ./tsconfig.json -t 1000000 --exit -b -j 1 tests/**/*.ts",
    "test:drift": "pnpm run ts-mocha -p ./tsconfig.json -t 1000000 --exit -b tests/driftVaults.ts",
    "test:phoenix": "pnpm run ts-mocha -p ./tsconfig.json -t 1000000 --exit -b tests/phoenixVaults.ts",
    "test:demo": "pnpm run ts-mocha -p ./tsconfig.json -t 1000000 --exit -b tests/demo.ts"
  },
